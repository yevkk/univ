@model IEnumerable<DDB_CW.Models.CheckIn>

@{
    ViewData["Title"] = "Check-ins";
}

<h1>Check-ins</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ts)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BookingId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
            var str = item.Booking != null ? item.Booking.Desc : "null";
            <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ts)
            </td>
            <td>
                @Html.DisplayFor(modelItem => str)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
